 
<body>
	<div class="wrapper">
		 

		<div class="main">
			 

			<main class="content">
				<div class="container-fluid p-0">

					<div class="mb-3">
						<h1 class="h3 d-inline align-middle">Admin Profile</h1>
		 </div>
					<div class="row">
						<div class="col-md-4 col-xl-3">
							<div class="card mb-3">
								<div class="card-header">
 								</div>
								<div class="card-body text-center">
									<img src="/public/pictures/uprofile2.webp" alt="Christina Mason" class="img-fluid rounded-circle mb-2" width="128" height="128" />
									<h5 class="card-title mb-0"> </h5>
									<div class="text-muted mb-2">Admin Profile </div>

								</div>
								 
							</div>
						</div>

						<div class="col-md-8 col-xl-9">
							<div class="card">
					
								<div class="card-body h-100">
    
                                    <form action="/dashboard/admin/add" method="post">
                                        <div class="card">
                                             <div class="card-body">
                                                 
											 
												<input type="text" required value="<%= //currenUsernInfo.username %>" class="form-control" name="username" placeholder="Enter username here">
												<br>
												<input type="text" required value="<%= //currenUsernInfo.tel_house%>" class="form-control" name="tel_house" placeholder="Enter house telepone  here">
												<br>
											   
												<input type="text" required value="<%= //currenUsernInfo.telephone%>" class="form-control" name="telephone" placeholder="Enter telephone here">
												
												<br>
												<input type="text" required value="<%= //currenUsernInfo.region%>" class="form-control" name="region" placeholder="Enter region here">
												<br>
												<input type="text" required value="<%= //currenUsernInfo.address %>" class="form-control" name="address" placeholder="Enter address here">
												<br>
												<input type="text" required value="<%= //currenUsernInfo.house_no%>" class="form-control" name="house_no" placeholder="Enter house number here">
												<br>
											   
												<input type="email" required value="<%= //currenUsernInfo.email%>" class="form-control" name="email" placeholder="Enter email here">
												
												<br>
												<input type="text" required value="<%= //currenUsernInfo.password%>" class="form-control" name="password" placeholder="Enter password here">
												<br>
												
												<input type="submit" class="submit" value="Add User">
											   
									
                                                   
										 
                                                <style>
                                                    .submit{
                                                        width:120px;
                                                        margin-top: 20px;
                                                        border: 1px solid rgb(197, 193, 193);
                                                        padding: 8px 12px;
                                                        border-radius: 2px;
                                                        color: rgb(55, 54, 54);
                                                    }
													.submit:hover{
														border: 1px solid rgb(40, 40, 40);
													}
                                                    .form-control{
                                                        height: 50px;
                                                    }
                                                </style>
    
                                            </div>
                                        </div>
    
                                    </form>
									
  
								</div>
							</div>
						</div>
					</div>

				</div>
			</main>
  
		</div>
	</div>

 
</body>

</html>