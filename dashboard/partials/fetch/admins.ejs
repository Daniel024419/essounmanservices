  
					<div class="row">
                        <style>
                            .row{
    width: 120%;
}
@media screen and (max-width:800px) {
     .row{
        width: auto !important;
     }
     th,td{
        font-size: 12.2px;
     }
}
                        </style>
						<div class="col-12 col-lg-8 col-xxl-9 d-flex">
							<div class="card flex-fill">
								<div class="card-header">

									<h5 class="card-title mb-0">
                                        <% if(result_admins!=undefined) {%> 
                                         Admins ( <%= result_admins.length %> )
                                         <% } %> 
                                        </h5>
								</div>

                                <div class="card-header">


                                    <form action="/dashboard/admin" method="get">                                                    <div class="card">
                                                        
                                        <div class="card-body">
                                            <% if(search!=undefined) {%>
                                                <input type="text"   
                                                value="<%=search.searchInput%>" 
                                                class="form-control" 
                                                name="searchInput" 
                                                placeholder="Enter search here and press enter key">
                                         
                                                <% } %>
                                                 
                                        </div>
                                    </div>

                                    </form>
                                
 
                           <% if(result_admins!=undefined){ %> 
                                            <% 
                                               if(result_admins.length==0) {%> 
                                                <br> <center><h3 style="color: red;">No returned results for the search</h3></center><br>
                                            <% } %>   
                                          <%   }%>
								  </div>
								<table class="table table-hover my-0">
									<thead>
										<tr>
											<th>#</th>
                                            <!-- <th>User Id</th> -->
                                            <th class="d-none d-xl-table-cell">Userame</th>
                                            <th class="d-none d-xl-table-cell">House Tel</th>
                                            <th>Tel</th>
                                            <th class="d-none d-xl-table-cell">Region</th>
                                            <th >Address</th>
                                            <th class="d-none d-xl-table-cell">House No.</th>
                                            <th class="d-none d-xl-table-cell">Email</th>
                                            <th class="d-none d-xl-table-cell">Password</th>
                                            <th >Date</th>
                                            <th>Action?</th>
                                            <!-- <th>Response</th> -->
											<!-- <th class="d-none d-xl-table-cell">End Date</th>
											<th>Status</th>
											<th class="d-none d-md-table-cell">Assignee</th> -->
										</tr>
									</thead>
									<tbody>
										 
                                        <% if(result_admins!=undefined) {%> 

                                        <% for( let index = 0; index < result_admins.length; index++ ) { %>


                                            <tr>
                                                <td><%= result_admins[index].id%></td>
                                                <!-- <td><%= //Users_admin[index].user_id%></td> -->
                                                 <td class="d-none d-xl-table-cell"><%= result_admins[index].username%></td>
                                                <td class="d-none d-xl-table-cell"> <a href="tel:<%= result_admins[index].tel_house %>"><i class="align-middle" data-feather="phone-call"></i></a></td>
                                                
                                                <td> <a href="tel:<%= result_admins[index].telephone%>">
                                                <i class="align-middle" data-feather="phone-call"></i></a> </td>
                                                <td class="d-none d-xl-table-cell"><%= result_admins[index].region%></td>
                                                <td><%= result_admins[index].address%></td>
                                                <td class="d-none d-xl-table-cell"><%= result_admins[index].house_no%></td>
                                                <td class="d-none d-xl-table-cell"><%= result_admins[index].email%></td>
                                                <td class="d-none d-xl-table-cell"><%= result_admins[index].password%></td>
                                                <td ><%= result_admins[index].date_created%></td>
                                                 <!-- <td><%= //Users_admin[index].response%></td> -->
                                                
                                                 <td>
                                                    <% if(result_admins[index].id==1) {%> 
                                                        
                                                        <span class="badge " style="background-color: rgb(2, 108, 14);padding: 10px;">
                                                            <a href="#"
                                                            style="color: white;padding: 2px;text-decoration: none;">??</a>
                                                        </span>
                                                    <% } else{%> 
                                                        <span class="badge " style="background-color: rgb(236, 4, 4);padding: 10px;">
                                                            <a onclick="warn();"   href="#"
                                                            style="color: white;padding: 2px;text-decoration: none;">Del</a>
                                                        </span>
                                                        <script>
                                                            function warn(){
                                                              const  conf= confirm('Do you want to delete this admin?');
                                                              if(conf==true){
                                                                window.location.href='/delete-admin/<%=result_admins[index].id%>';
                                                                return true;
                                                              }
                                                              else{
                                                                return false;
                                                              }
                                                            }
                                                        </script>
                                                    <% } %> 
                                                </td>
                                               
                                                <!-- <td class="d-none d-xl-table-cell">01/01/2021</td>
                                                <td class="d-none d-xl-table-cell">31/06/2021</td>
                                                <td><span class="badge bg-warning">In progress</span></td>
                                                <td class="d-none d-md-table-cell">William Harris</td> -->
                                            </tr>
                                        <% } %>


                                        <% } else{%>  

                                            <tr colspan="4">
                                                <td class="d-none d-md-table-cell">You do not have any users yet..</td>
                                            </tr>
                                            <%  }%> 



									</tbody>
								</table>
							</div>
						</div>
						 
					</div>

			 

	 

 
 