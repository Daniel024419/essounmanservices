  
					<div class="row">
                        <style>
                            .row{
    width: 120%;
}
@media screen and (max-width:800px) {
     .row{
        width: auto !important;
     }
     th,td{
        font-size: 12.2px;
     }
}
                        </style>
						<div class="col-12 col-lg-8 col-xxl-9 d-flex">
            
                                
							<div class="card flex-fill">
								<div class="card-header">


                                    <form action="/dashboard/orders" method="get">                                                    <div class="card">
                                                        
                                        <div class="card-body">
                                            <% if(search!=undefined) {%>
                                                <input type="text"   value="<%=search.searchInput%>" 
                                                class="form-control" name="searchInput" placeholder="Enter search here">
                                         
                                                <% } %>
                                                 
                                        </div>
                                    </div>

                                    </form>
                                   <%  if(orders_result_admin.length!=undefined){ %> 
