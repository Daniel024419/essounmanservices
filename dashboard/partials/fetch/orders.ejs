                                        <%  }%>  
 
                                             

                                                
                                               
                                            


                                        
                                         <% if(orders_result_admin!=undefined){ %> 
                                            <% 
                                               if(orders_result_admin.length==0) {%> 
                                                <br> <center><h3 style="color: red;">No returned results for the search</h3></center><br>
                                            <% } %>   
                                          <%   }%>
								  </div>

                                   
								  <table class="table table-hover my-0">
									<thead>
										<tr>
											<th style="width: 8px;">#</th>
