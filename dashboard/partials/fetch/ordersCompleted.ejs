                                                 <td>
                                                    <span class="badge " style="background-color: rgb(236, 19, 4);padding: 10px;">
                                                        <a href="#"  onclick="warn();"
                                                             style="color: white;padding: 2px;text-decoration: none;">Del</a>
                                                    </span>
                                                </td>
                                               

                                                <script>
                                                    function warn(){
                                                      const  conf= confirm('Do you want to delete this\nOnce deleted can not be recovered');
                                                      if(conf==true){
                                                        window.location.href='/order-delete/<%=orders_result_admin_completed[index].id%>';
                                                         return true;
                                                      }
                                                      else{
                                                        return false;
                                                      }
                                                    }
                                                </script>

                                                <!-- <td class="d-none d-xl-table-cell">01/01/2021</td>
                                                <td class="d-none d-xl-table-cell">31/06/2021</td>
                                                <td><span class="badge bg-warning">In progress</span></td>
                                                <td class="d-none d-md-table-cell">William Harris</td> -->
                                            </tr>
                                        <% } %>


                                        <% } else{%>  

                                            <tr colspan="4">
                                                <td class="d-none d-md-table-cell">You do not have any orders yet..</td>
                                            </tr>
                                            <%  }%> 



									</tbody>
								</table>
							</div>
						</div>
						 
					</div>

			 

	 

 
 