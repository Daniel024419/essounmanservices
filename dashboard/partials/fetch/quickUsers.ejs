                                        <div class="row">
						<div class="col-12 col-lg-8 col-xxl-9 d-flex">
							<div class="card flex-fill">
								<div class="card-header">

									<h5 class="card-title mb-0">Quick Users (<%= result_quickuers_admins.length %> )</h5>
								</div>
                                <div class="card-header">


                                    <form action="/dashboard/quick-users" method="get">                                                    <div class="card">
                                                        
                                        <div class="card-body">
                                            <% if(search!=undefined) {%>
                                                <input type="text"   
                                                value="<%=search.searchInput%>" 
                                                class="form-control" 
                                                name="searchInput" 
                                                placeholder="Enter search here and press enter key">
                                         
                                                <% } %>
                                                 
                                        </div>
                                    </div>

                                    </form>
                                
 
                                  <% if(result_quickuers_admins!=undefined){ %> 
                                            <% 
                                               if(result_quickuers_admins.length==0) {%> 
                                                <br> <center><h3 style="color: red;">No returned results for the search</h3></center><br>
                                            <% } %>   
                                          <%   }%>
								  </div>
								<table class="table table-hover my-0">
									<thead>
										<tr>
											<th>#</th>
                                            <!-- <th>User Id</th> -->
                                            <th >Userame</th>
                                            <th class="d-none d-xl-table-cell">House Tel</th>
                                            <th>Tel</th>
                                            <th class="d-none d-xl-table-cell">Region</th>
                                            <th >Address</th>
                                            <th class="d-none d-xl-table-cell">House No.</th>
                                            <th class="d-none d-xl-table-cell">Email</th>
                                            <th class="d-none d-xl-table-cell">Password</th>
                                            <th class="d-none d-xl-table-cell">Date</th>
                                            <th>Action?</th>
                                            <!-- <th>Response</th> -->
											<!-- <th class="d-none d-xl-table-cell">End Date</th>
											<th>Status</th>
											<th class="d-none d-md-table-cell">Assignee</th> -->
										</tr>
									</thead>
									<tbody>
										 
                                        <% if(result_quickuers_admins!=undefined) {%> 

                                        <% for( let index = 0; index < result_quickuers_admins.length; index++ ) { %>


                                            <tr>
                                                <td><%= result_quickuers_admins[index].id%></td>
                                                <!-- <td><%= //Users_admin[index].user_id%></td> -->
                                                 <td ><%= result_quickuers_admins[index].user_name%></td>
                                                <td class="d-none d-xl-table-cell"><a href="<%= result_quickuers_admins[index].Tel_house%>"><i class="align-middle" data-feather="phone-call"></i></a> </td>
                                                <td> <a href="tel:<%= result_quickuers_admins[index].telephone%>"><i class="align-middle" data-feather="phone-call"></i></a> </td>
                                                <td class="d-none d-xl-table-cell"><%= result_quickuers_admins[index].region%></td>
                                                <td><%= result_quickuers_admins[index].address%></td>
                                                <td class="d-none d-xl-table-cell"><%= result_quickuers_admins[index].house_no%></td>
                                                <td class="d-none d-xl-table-cell"><%= result_quickuers_admins[index].email%></td>
                                                <td class="d-none d-xl-table-cell"><%= result_quickuers_admins[index].password%></td>
                                                <td class="d-none d-xl-table-cell"><%= result_quickuers_admins[index].date%></td>
                                                
                                                 <td>
                                                    <span class="badge " style="background-color: rgb(236, 19, 4);padding: 10px;">
                                                        <a href="#" onclick="warn();"
                                                             style="color: white;padding: 2px;text-decoration: none;">Del</a>
                                                    </span>
                                                </td>
                                                <script>
                                                    function warn(){
                                                      const  conf= confirm('Do you want to delete this user?');
                                                      if(conf==true){
                                                        window.location.href='/delete-quick-users/<%=result_quickuers_admins[index].id%>';
                                                        return true;
                                                      }
                                                      else{
                                                        return false;
                                                      }
                                                    }
                                                </script>
                                                <!-- <td class="d-none d-xl-table-cell">01/01/2021</td>
                                                <td class="d-none d-xl-table-cell">31/06/2021</td>
                                                <td><span class="badge bg-warning">In progress</span></td>
                                                <td class="d-none d-md-table-cell">William Harris</td> -->
                                            </tr>
                                        <% } %>


                                        <% } else{%>  

                                            <tr colspan="4">
                                                <td class="d-none d-md-table-cell">You do not have any users yet..</td>
                                            </tr>
                                            <%  }%> 



									</tbody>
								</table>
							</div>
						</div>
						 
					</div>

			 

	 

 
 