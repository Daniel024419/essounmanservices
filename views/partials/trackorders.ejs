 <!-- 1 -->
     
    <% if(order_result!=undefined) {%> 
   
   <% for( let index = 0; index < order_result.length; index++ ) { %>
       <div class="columns">
           <ul class="price">
               
           <li class="header" style="text-transform: uppercase;"><%= order_result[index].package%></li>
           <table width="100%" class="table-service">
                   <!-- thead -->
                   <thead>
                   <th>Price</th>
                   <th>Type</th>
                   </thead>
                   <!-- end -->
   
                   <!-- tr -->
               <tr>
                    
        <td><li class="grey"> &#8373<%=order_result[index].price%></li></td>
                   
        <td><li class="grey"><%= order_result[index].type %></li></td>
                   </tr>
                   <!-- end -->
   
           <tr class="" style="display: none;">
           <th colspan="2" style="text-align: center;">Add-ons</th>
           </tr> 
   
   
           <tr class="addon">
               <th colspan="2" style="text-align: center;">Booking Info</th>
           </tr>
   <!-- date -->
             <tr>
              <td><li class="grey">Time: </li></td> 
              <td><li class="grey"><%= order_result[index].book_date %></li></td> 
             </tr>
              
   <!-- location -->
       <tr>
       <td><li class="grey">Address: </li></td> 
       <td><li class="grey"><%= order_result[index].address %></li></td> 
       </tr>
   
       <!-- end -->
       <!-- location -->
       <tr>
           <td><li class="grey">Contact: </li></td> 
           <td><li class="grey"><%= order_result[index].Tel_house %></li></td> 
           </tr>
       
           <!-- end -->
           </table>
          <!--  -->
   
   
           <li class="grey" >
           <!-- <a href="/delete-order/<%=//order_result[index].id; %>" style="background-color: red;" class="button"> -->
               <a href="#" onclick="warn();" style="background-color: red;" class="button">
   
               <i class="fa fa-trash" style="padding-right: 5px;"></i>Delete</a>
           <a href="/booking/service/edit/<%=order_result[index].id; %>" style="background-color: rgb(4, 237, 140);" class="button">
               <i class="fa fa-edit" style="padding: 5px;"></i>Edit Info</a>
               
           </li>
           <script>
               function warn(){
                 const  conf= confirm('Do you want to delete this');
                 if(conf==true){
                   window.location.href='/delete-order/<%=order_result[index].id; %>';
                   return true;
                 }
                 else{
                   return false;
                 }
               }
           </script>
           
           </ul>
       </div> 
       <% } %>

       <% if(order_result.length==0) {%>
        <center>
            <h2 style="padding: 10px;color: red;">
                Pls check your email and booking Id.... <a href="/track-orders">Try Again</a>
            </h2>
        </center>
        <% } %>  
       <% } else{ %> 
           <script>
               // window.location.href='/';
           </script>
           <center>
               <h1>You do not have any orders yet..</h1>
           </center>
       <% } %> 
       